  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.1",
    "react-router": "4.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.1",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-system-import-commonjs": "^1.0.3",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "webpack": "2.2.0"
