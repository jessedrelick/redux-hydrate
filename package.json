{
  "name": "redux-hydration",
  "version": "0.5.0",
  "description": "Simple library for hydrating Redux state on the server",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node example/index.js --plugins=transform-system-import-commonjs",
    "build": "npm run build:package && npm run build:example",
    "build:package": "./node_modules/.bin/babel src --out-dir dist",
    "build:example": "./node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jessedrelick/redux-hydration.git"
  },
